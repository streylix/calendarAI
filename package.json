{
  "name": "ai-calendar",
  "version": "1.0.0",
  "description": "AI-powered desktop calendar application",
  "main": "main/main.js",
  "scripts": {
    "start": "cross-env ELECTRON_START_URL=http://localhost:3000 electron .",
