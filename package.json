    "openai": "^4.98.0",
    "react": "^19.1.0",
    "react-big-calendar": "^1.18.0",
    "react-dom": "^19.1.0",
    "tailwindcss": "^4.1.6"
  },
  "devDependencies": {
    "electron": "^36.2.0",
    "electron-builder": "^26.0.12",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "wait-on": "^8.0.3",
    "react-scripts": "^5.0.1"
  },
  "browserslist": {
